{{!-- Create a table --}}
<table>
	{{!-- Header section --}}
	<thead>
		<tr>
			{{#each data.[0]}}
			<th>
				{{@key}}
			</th>
			{{/each}}
	</thead>
	{{!-- Body section --}}
	<tbody>
		{{!-- For each row, print the id, character name, and stats --}}
		{{#each data}}
		<tr>
			<td>{{this.character_id}}</td>
		</tr>
	</tbody>
</table>
